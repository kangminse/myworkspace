package quzi;

import java.util.HashMap;
import java.util.Map.Entry;

public class Win10Service {
	Win10DTO dto;
	public Win10Service() {
		dto = new Win10DTO();
		HashMap<String, String> map = new HashMap<>();
		map.put("RAM", "16기가");
		map.put("시스템 종류", "64비트");
		dto.setInfo(map);
		
	}
	
	public void display() {
		System.out.println(dto.getInfo());
	}
	
}
