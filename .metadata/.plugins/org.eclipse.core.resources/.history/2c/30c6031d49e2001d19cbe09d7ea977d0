package object;

import java.util.Scanner;

public class TestClass06 {
	
	public int[] myInput() {
		Scanner input = new Scanner(System.in);
		int n1, n2, sum;
		System.out.print("수 입력 : ");
		n1 = input.nextInt();
		
		System.out.print("수 입력 : ");
		n2 = input.nextInt();
		int[] arr = {n1, n2};
 		return arr;
	}
	
	public void test(int n1, int n2) {
		int sum = n1 + n2;
	}
	
	public void print() {
		System.out.println(n1 + "+" + n2 + "=" + sum);
	}
	
}
