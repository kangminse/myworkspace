package singleton;

class A03{
	static A03 a;
	
	private A03(){
		System.out.println("생성자 실행");
	}
	public static A03 getInstance() {
		System.out.println(a);
		return new A03();
	}
}

public class MainClass03 {
	public static void main(String[] args) {
		A03 a01 = A03.getInstance();
	}
}











