package inheritance;

class car{
	public void basic() {
		System.out.println("기본기능");
	}
	
	public void Car() {
		System.out.println("자동차.");
	}

}

class outocar extends car{
	public void OutoCar() {
		System.out.println("자율 주행 자동차");
	}
	
}

class divecar extends car{
	public void DiveCar() {
		System.out.println("수중 자동차");
	}
}


public class test {
	public static void main(String[] args) {
		car c;
		c = new divecar();
		c = new outocar();
		
		System.out.println(c);
	}
}
