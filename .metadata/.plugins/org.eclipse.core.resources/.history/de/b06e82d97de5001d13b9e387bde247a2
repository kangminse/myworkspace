package inheritance;

class car{
	public void basic() {
		System.out.println("기본기능");
	}
	
	public void Car() {
		System.out.println("자동차");
	}

}

class outocar extends car{
	public void outoDrive() {
		System.out.println("자율 주행 자동차");
	}
	
}

class divecar extends car{
	public void DiveCar() {
		System.out.println("수중 자동차");
	}
}


public class test {
	public static void main(String[] args) {
		outocar a = new outocar();
		car b = new outocar();
		
		divecar c = new divecar();
		car d = new divecar();
		
		
	
	}
}
